{
  "$schema": "https://json.schemastore.org/swcrc",
  "jsc": {
    "parser": { "syntax": "typescript" },
    "transform": { "react": { "useBuiltins": true } },
    "target": "es2022"
  },
  "module": {
    "type": "es6"
  },
  "exclude": [
    "src/styles.js",
    "src/styles.d.ts",
    // Since swc converts `react-table-types.d.ts` to `react-table-types.d.js`
    "src/core/Table/types/react-table-types.d.ts"
  ]
}
