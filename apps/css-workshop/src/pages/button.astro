---
import Layout from './_layout.astro';
import Button_ from '../components/Button.astro';
import IconButton_ from '../components/IconButton.astro';
---

<Layout title='Button'>
  <script>
    import '@itwin/itwinui-icons-elements/placeholder';
    import '@itwin/itwinui-icons-elements/caret-down-small';
    import '@itwin/itwinui-icons-elements/download';
    import '@itwin/itwinui-icons-elements/arrow-right';
    import '@itwin/itwinui-icons-elements/delete';
    import '@itwin/itwinui-icons-elements/rename';
    import '@itwin/itwinui-icons-elements/more';
    import '@itwin/itwinui-icons-elements/list';
    import '@itwin/itwinui-icons-elements/thumbnails';
    import '@itwin/itwinui-icons-elements/smiley-happy';
    import '@itwin/itwinui-icons-elements/close-small';
  </script>
  <style is:global>
    #demo-default button,
    #demo-high-visibility button,
    #demo-cta button,
    #demo-disabled button,
    #demo-borderless button,
    #demo-split .iui-button-split {
      margin-right: 4px;
    }

    .demo-container,
    .iui-button-group,
    .iui-button-split,
    h3 {
      margin-bottom: 12px;
    }

    .demo-container .iui-button {
      margin-right: 4px;
    }

    #demo-default button,
    #demo-borderless button,
    #demo-grouped span {
      margin-bottom: 12px;
    }
  </style>

  <h1>Button</h1>
  <hr />

  <h2>Default</h2>

  <h3>High visibility</h3>

  <section id='demo-high-visibility'>
    <div id='demo-high-visibility-small' class='demo-container'>
      <Button_ variant='high-visibility' size='small'>Small</Button_>
      <Button_ variant='high-visibility' size='small'>
        <svg-placeholder aria-hidden='true' slot='start-icon'></svg-placeholder>
         Small left icon
      </Button_>
      <Button_ variant='high-visibility' size='small'>
        Small right icon
        <svg-placeholder aria-hidden='true' slot='end-icon'></svg-placeholder>
      </Button_>
      <IconButton_ variant='high-visibility' size='small'>
        <svg-placeholder></svg-placeholder>
      </IconButton_>
      <Button_ variant='high-visibility' size='small' disabled>
        <svg-placeholder aria-hidden='true' slot='start-icon'></svg-placeholder>
         Small disabled
      </Button_>
    </div>

    <div id='demo-high-visibility-normal' class='demo-container'>
      <Button_ variant='high-visibility'>Normal</Button_>
      <Button_ variant='high-visibility'>
        <svg-placeholder aria-hidden='true' slot='start-icon'></svg-placeholder>
         Normal left icon
      </Button_>
      <Button_ variant='high-visibility'>
        Normal right icon
        <svg-placeholder aria-hidden='true' slot='end-icon'></svg-placeholder>
      </Button_>
      <IconButton_ variant='high-visibility'>
        <svg-placeholder></svg-placeholder>
      </IconButton_>
      <Button_ variant='high-visibility' disabled>
        <svg-placeholder aria-hidden='true' slot='start-icon'></svg-placeholder>
         Normal disabled
      </Button_>
    </div>

    <div id='demo-high-visibility-large' class='demo-container'>
      <Button_ variant='high-visibility' size='large'>Large</Button_>
      <Button_ variant='high-visibility' size='large'>
        <svg-placeholder aria-hidden='true' slot='start-icon'></svg-placeholder>
         Large left icon
      </Button_>
      <Button_ variant='high-visibility' size='large'>
        Large right icon
        <svg-placeholder aria-hidden='true' slot='end-icon'></svg-placeholder>
      </Button_>
      <IconButton_ variant='high-visibility' size='large'>
        <svg-placeholder></svg-placeholder>
      </IconButton_>
      <Button_ variant='high-visibility' size='large' disabled>
        <svg-placeholder aria-hidden='true' slot='start-icon'></svg-placeholder>
         Large disabled
      </Button_>
    </div>
  </section>

  <h3>Call to action</h3>
  <section id='demo-cta'>
    <div id='demo-cta-small' class='demo-container'>
      <Button_ variant='cta' size='small'>Small</Button_>
      <Button_ variant='cta' size='small'>
        <svg-placeholder aria-hidden='true' slot='start-icon'></svg-placeholder>
         Small left icon
      </Button_>
      <Button_ variant='cta' size='small'>
        Small right icon
        <svg-placeholder aria-hidden='true' slot='end-icon'></svg-placeholder>
      </Button_>
      <IconButton_ variant='cta' size='small'>
        <svg-placeholder></svg-placeholder>
      </IconButton_>
      <Button_ variant='cta' size='small' disabled>
        <svg-placeholder aria-hidden='true' slot='start-icon'></svg-placeholder>
         Small disabled
      </Button_>
    </div>

    <div id='demo-cta-medium' class='demo-container'>
      <Button_ variant='cta'>Normal</Button_>
      <Button_ variant='cta'>
        <svg-placeholder aria-hidden='true' slot='start-icon'></svg-placeholder>
         Normal left icon
      </Button_>
      <Button_ variant='cta'>
        Normal right icon
        <svg-placeholder aria-hidden='true' slot='end-icon'></svg-placeholder>
      </Button_>
      <IconButton_ variant='cta'>
        <svg-placeholder></svg-placeholder>
      </IconButton_>
      <Button_ variant='cta' disabled>
        <svg-placeholder aria-hidden='true' slot='start-icon'></svg-placeholder>
         Normal disabled
      </Button_>
    </div>

    <div id='demo-cta-large' class='demo-container'>
      <Button_ variant='cta' size='large'>Large</Button_>
      <Button_ variant='cta' size='large'>
        <svg-placeholder aria-hidden='true' slot='start-icon'></svg-placeholder>
         Large left icon
      </Button_>
      <Button_ variant='cta' size='large'>
        Large right icon
        <svg-placeholder aria-hidden='true' slot='end-icon'></svg-placeholder>
      </Button_>
      <IconButton_ variant='cta' size='large'>
        <svg-placeholder></svg-placeholder>
      </IconButton_>
      <Button_ variant='cta' size='large' disabled>
        <svg-placeholder aria-hidden='true' slot='start-icon'></svg-placeholder>
         Large disabled
      </Button_>
    </div>
  </section>

  <h3>Default</h3>
  <section id='demo-default'>
    <div id='demo-default-small' class='demo-container'>
      <Button_ size='small'>Small</Button_>
      <Button_ size='small'>
        <svg-placeholder aria-hidden='true' slot='start-icon'></svg-placeholder>
         Small left icon
      </Button_>
      <Button_ size='small'>
        Small right icon
        <svg-placeholder aria-hidden='true' slot='end-icon'></svg-placeholder>
      </Button_>
      <IconButton_ size='small'>
        <svg-placeholder></svg-placeholder>
      </IconButton_>
      <Button_ dropdown size='small'>
        Small dropdown
        <svg-caret-down-small aria-hidden='true' slot='end-icon'></svg-caret-down-small>
      </Button_>
      <Button_ size='small' disabled>
        <svg-placeholder aria-hidden='true' slot='start-icon'></svg-placeholder>
         Small disabled
      </Button_>
    </div>

    <div id='demo-default-medium' class='demo-container'>
      <Button_>Normal</Button_>
      <Button_>
        <svg-placeholder aria-hidden='true' slot='start-icon'></svg-placeholder>
         Normal left icon
      </Button_>
      <Button_>
        Normal right icon
        <svg-placeholder aria-hidden='true' slot='end-icon'></svg-placeholder>
      </Button_>
      <IconButton_>
        <svg-placeholder></svg-placeholder>
      </IconButton_>
      <Button_ dropdown>
        Normal dropdown
        <svg-caret-down-small aria-hidden='true' slot='end-icon'></svg-caret-down-small>
      </Button_>
      <Button_ disabled>
        <svg-placeholder aria-hidden='true' slot='start-icon'></svg-placeholder>
         Normal disabled
      </Button_>
    </div>

    <div id='demo-default-large' class='demo-container'>
      <Button_ size='large'>Large</Button_>
      <Button_ size='large'>
        <svg-placeholder aria-hidden='true' slot='start-icon'></svg-placeholder>
         Large left icon
      </Button_>
      <Button_ size='large'>
        Large right icon
        <svg-placeholder aria-hidden='true' slot='end-icon'></svg-placeholder>
      </Button_>
      <IconButton_ size='large'>
        <svg-placeholder></svg-placeholder>
      </IconButton_>
      <Button_ dropdown size='large'>
        Large dropdown
        <svg-caret-down-small aria-hidden='true' slot='end-icon'></svg-caret-down-small>
      </Button_>
      <Button_ size='large' disabled>
        <svg-placeholder aria-hidden='true' slot='start-icon'></svg-placeholder>
         Large disabled
      </Button_>
    </div>
  </section>

  <h3>Borderless</h3>
  <section id='demo-borderless'>
    <div id='demo-borderless-small' class='demo-container'>
      <Button_ variant='borderless' size='small'>Small</Button_>
      <Button_ variant='borderless' size='small'>
        <svg-placeholder aria-hidden='true' slot='start-icon'></svg-placeholder>
         Small left icon
      </Button_>
      <Button_ variant='borderless' size='small'>
        Small right icon
        <svg-placeholder aria-hidden='true' slot='end-icon'></svg-placeholder>
      </Button_>
      <IconButton_ variant='borderless' size='small'>
        <svg-placeholder></svg-placeholder>
      </IconButton_>
      <Button_ dropdown variant='borderless' size='small'>
        Small dropdown
        <svg-caret-down-small aria-hidden='true' slot='end-icon'></svg-caret-down-small>
      </Button_>
      <Button_ variant='borderless' size='small' disabled>
        <svg-placeholder aria-hidden='true' slot='start-icon'></svg-placeholder>
         Small disabled
      </Button_>
    </div>

    <div id='demo-borderless-medium' class='demo-container'>
      <Button_ variant='borderless'>Normal</Button_>
      <Button_ variant='borderless'>
        <svg-placeholder aria-hidden='true' slot='start-icon'></svg-placeholder>
         Normal left icon
      </Button_>
      <Button_ variant='borderless'>
        Normal right icon
        <svg-placeholder aria-hidden='true' slot='end-icon'></svg-placeholder>
      </Button_>
      <IconButton_ variant='borderless'>
        <svg-placeholder></svg-placeholder>
      </IconButton_>
      <Button_ dropdown variant='borderless'>
        Normal dropdown
        <svg-caret-down-small aria-hidden='true' slot='end-icon'></svg-caret-down-small>
      </Button_>
      <Button_ variant='borderless' disabled>
        <svg-placeholder aria-hidden='true' slot='start-icon'></svg-placeholder>
         Normal disabled
      </Button_>
    </div>

    <div id='demo-borderless-large' class='demo-container'>
      <Button_ variant='borderless' size='large'>Large</Button_>
      <Button_ variant='borderless' size='large'>
        <svg-placeholder aria-hidden='true' slot='start-icon'></svg-placeholder>
         Large left icon
      </Button_>
      <Button_ variant='borderless' size='large'>
        Large right icon
        <svg-placeholder aria-hidden='true' slot='end-icon'></svg-placeholder>
      </Button_>
      <IconButton_ variant='borderless' size='large'>
        <svg-placeholder></svg-placeholder>
      </IconButton_>
      <Button_ dropdown variant='borderless' size='large'>
        Large dropdown
        <svg-caret-down-small aria-hidden='true' slot='end-icon'></svg-caret-down-small>
      </Button_>
      <Button_ variant='borderless' size='large' disabled>
        <svg-placeholder aria-hidden='true' slot='start-icon'></svg-placeholder>
         Large disabled
      </Button_>
    </div>
  </section>

  <hr />

  <h2>With notifications</h2>
  <section id='demo-notification'>
    <div id='demo-notification-small' class='demo-container'>
      <Button_ variant='high-visibility' size='small'>
        <span
          class='iui-notification-marker'
          data-iui-variant='white'
          aria-hidden='true'
          slot='start-icon'
        >
          <svg-placeholder aria-hidden='true'></svg-placeholder>
        </span>
         Notification
      </Button_>
      <Button_ variant='cta' size='small'>
        <span
          class='iui-notification-marker'
          data-iui-variant='white'
          aria-hidden='true'
          slot='start-icon'
        >
          <svg-placeholder aria-hidden='true'></svg-placeholder>
        </span>
         Notification
      </Button_>
      <Button_ size='small'>
        <span
          class='iui-notification-marker'
          data-iui-variant='informational'
          aria-hidden='true'
          slot='start-icon'
        >
          <svg-placeholder aria-hidden='true'></svg-placeholder>
        </span>
         Notification
      </Button_>
      <Button_ variant='borderless' size='small'>
        <span
          class='iui-notification-marker'
          data-iui-variant='primary'
          aria-hidden='true'
          slot='start-icon'
        >
          <svg-placeholder aria-hidden='true'></svg-placeholder>
        </span>
         Notification
      </Button_>
    </div>

    <div id='demo-notification-medium' class='demo-container'>
      <Button_ variant='high-visibility'>
        <span
          class='iui-notification-marker'
          data-iui-variant='white'
          aria-hidden='true'
          slot='start-icon'
        >
          <svg-placeholder aria-hidden='true'></svg-placeholder>
        </span>
         Notification
      </Button_>
      <Button_ variant='cta'>
        <span
          class='iui-notification-marker'
          data-iui-variant='white'
          aria-hidden='true'
          slot='start-icon'
        >
          <svg-placeholder aria-hidden='true'></svg-placeholder>
        </span>
         Notification
      </Button_>
      <Button_>
        <span
          class='iui-notification-marker'
          data-iui-variant='informational'
          aria-hidden='true'
          slot='start-icon'
        >
          <svg-placeholder aria-hidden='true'></svg-placeholder>
        </span>
         Notification
      </Button_>
      <Button_ variant='borderless'>
        <span
          class='iui-notification-marker'
          data-iui-variant='primary'
          aria-hidden='true'
          slot='start-icon'
        >
          <svg-placeholder aria-hidden='true'></svg-placeholder>
        </span>
         Notification
      </Button_>
    </div>

    <div id='demo-notification-large' class='demo-container'>
      <Button_ variant='high-visibility' size='large'>
        <span
          class='iui-notification-marker'
          data-iui-variant='white'
          aria-hidden='true'
          slot='start-icon'
        >
          <svg-placeholder aria-hidden='true'></svg-placeholder>
        </span>
         Notification
      </Button_>
      <Button_ variant='cta' size='large'>
        <span
          class='iui-notification-marker'
          data-iui-variant='white'
          aria-hidden='true'
          slot='start-icon'
        >
          <svg-placeholder aria-hidden='true'></svg-placeholder>
        </span>
         Notification
      </Button_>
      <Button_ size='large'>
        <span
          class='iui-notification-marker'
          data-iui-variant='informational'
          aria-hidden='true'
          slot='start-icon'
        >
          <svg-placeholder aria-hidden='true'></svg-placeholder>
        </span>
         Notification
      </Button_>
      <Button_ variant='borderless' size='large'>
        <span
          class='iui-notification-marker'
          data-iui-variant='primary'
          aria-hidden='true'
          slot='start-icon'
        >
          <svg-placeholder aria-hidden='true'></svg-placeholder>
        </span>
         Notification
      </Button_>
    </div>
  </section>

  <hr />

  <h2>Grouped</h2>

  <h3>Default</h3>
  <section id='demo-grouped-default'>
    <span class='iui-button-group'>
      <Button_ size='small'>Left</Button_>
      <Button_ size='small'>Middle</Button_>
      <Button_ size='small'>Right</Button_>
    </span>

    <br />

    <span class='iui-button-group'>
      <Button_>Left</Button_>
      <Button_>Middle</Button_>
      <Button_>Right</Button_>
    </span>

    <br />

    <span class='iui-button-group'>
      <Button_ size='large'>Left</Button_>
      <Button_ size='large'>Middle</Button_>
      <Button_ size='large'>Right</Button_>
    </span>

    <br />

    <span class='iui-button-group'>
      <IconButton_ title='Download'>
        <svg-download></svg-download>
      </IconButton_>
      <IconButton_ title='Move'>
        <svg-arrow-right></svg-arrow-right>
      </IconButton_>
      <IconButton_ title='Delete' disabled>
        <svg-delete></svg-delete>
      </IconButton_>
      <IconButton_ title='Rename'>
        <svg-rename></svg-rename>
      </IconButton_>
      <IconButton_ title='More options…'>
        <svg-more></svg-more>
      </IconButton_>
    </span>

    <br />

    <span class='iui-button-group'>
      <IconButton_ disabled title='Download'>
        <svg-download></svg-download>
      </IconButton_>
      <IconButton_ disabled title='Move'>
        <svg-arrow-right></svg-arrow-right>
      </IconButton_>
      <IconButton_ title='Delete' disabled>
        <svg-delete></svg-delete>
      </IconButton_>
      <IconButton_ disabled title='Rename'>
        <svg-rename></svg-rename>
      </IconButton_>
      <IconButton_ disabled title='More options…'>
        <svg-more></svg-more>
      </IconButton_>
    </span>

    <br />

    <span class='iui-button-group'>
      <IconButton_ title='List view'>
        <svg-list></svg-list>
      </IconButton_>
      <IconButton_ title='Tile view' isActive>
        <svg-thumbnails></svg-thumbnails>
      </IconButton_>
    </span>

    <br />

    <span class='iui-button-group'>
      <IconButton_ disabled title='List view'>
        <svg-list></svg-list>
      </IconButton_>
      <IconButton_ disabled title='Tile view' isActive>
        <svg-thumbnails></svg-thumbnails>
      </IconButton_>
    </span>

    <br />

    <span class='iui-button-group' data-iui-orientation='vertical'>
      <IconButton_>
        <svg-download></svg-download>
      </IconButton_>
      <IconButton_>
        <svg-arrow-right></svg-arrow-right>
      </IconButton_>
      <IconButton_ disabled>
        <svg-delete></svg-delete>
      </IconButton_>
      <IconButton_ disabled>
        <svg-rename></svg-rename>
      </IconButton_>
      <IconButton_>
        <svg-more></svg-more>
      </IconButton_>
    </span>

    <span class='iui-button-group' data-iui-orientation='vertical'>
      <IconButton_>
        <svg-list></svg-list>
      </IconButton_>
      <IconButton_ isActive>
        <svg-thumbnails></svg-thumbnails>
      </IconButton_>
    </span>
  </section>

  <br />

  <h3>Borderless</h3>
  <section id='demo-grouped-borderless'>
    <span class='iui-button-group'>
      <Button_ variant='borderless' size='small'>Left</Button_>
      <Button_ variant='borderless' size='small'>Middle</Button_>
      <Button_ variant='borderless' size='small'>Right</Button_>
    </span>

    <br />

    <span class='iui-button-group'>
      <Button_ variant='borderless'>Left</Button_>
      <Button_ variant='borderless'>Middle</Button_>
      <Button_ variant='borderless'>Right</Button_>
    </span>

    <br />

    <span class='iui-button-group'>
      <Button_ variant='borderless' size='large'>Left</Button_>
      <Button_ variant='borderless' size='large'>Middle</Button_>
      <Button_ variant='borderless' size='large'>Right</Button_>
    </span>

    <br />

    <span class='iui-button-group'>
      <IconButton_ title='Download' variant='borderless'>
        <svg-download></svg-download>
      </IconButton_>
      <IconButton_ title='Move' variant='borderless'>
        <svg-arrow-right></svg-arrow-right>
      </IconButton_>
      <IconButton_ title='Delete' variant='borderless' disabled>
        <svg-delete></svg-delete>
      </IconButton_>
      <IconButton_ title='Rename' variant='borderless'>
        <svg-rename></svg-rename>
      </IconButton_>
      <IconButton_ title='More options…' variant='borderless'>
        <svg-more></svg-more>
      </IconButton_>
    </span>

    <br />

    <span class='iui-button-group'>
      <IconButton_ disabled title='Download' variant='borderless'>
        <svg-download></svg-download>
      </IconButton_>
      <IconButton_ disabled title='Move' variant='borderless'>
        <svg-arrow-right></svg-arrow-right>
      </IconButton_>
      <IconButton_ title='Delete' variant='borderless' disabled>
        <svg-delete></svg-delete>
      </IconButton_>
      <IconButton_ disabled title='Rename' variant='borderless'>
        <svg-rename></svg-rename>
      </IconButton_>
      <IconButton_ disabled title='More options…' variant='borderless'>
        <svg-more></svg-more>
      </IconButton_>
    </span>

    <br />

    <span class='iui-button-group'>
      <IconButton_ title='List view' variant='borderless'>
        <svg-list></svg-list>
      </IconButton_>
      <IconButton_ title='Tile view' variant='borderless' isActive>
        <svg-thumbnails></svg-thumbnails>
      </IconButton_>
    </span>

    <br />

    <span class='iui-button-group'>
      <IconButton_ disabled title='List view' variant='borderless'>
        <svg-list></svg-list>
      </IconButton_>
      <IconButton_ disabled title='Tile view' variant='borderless' isActive>
        <svg-thumbnails></svg-thumbnails>
      </IconButton_>
    </span>
  </section>

  <br />

  <h3>Split menu</h3>
  <section id='demo-split'>
    <span class='iui-button-split'>
      <Button_ variant='high-visibility' size='small'>Small split menu</Button_>
      <IconButton_ variant='high-visibility' size='small'>
        <svg-caret-down-small></svg-caret-down-small>
      </IconButton_>
    </span>
    <span class='iui-button-split'>
      <Button_ variant='cta' size='small'>Small split menu</Button_>
      <IconButton_ variant='cta' size='small'>
        <svg-caret-down-small></svg-caret-down-small>
      </IconButton_>
    </span>
    <span class='iui-button-split'>
      <Button_ size='small'>Small split menu</Button_>
      <IconButton_ size='small'>
        <svg-caret-down-small></svg-caret-down-small>
      </IconButton_>
    </span>
    <span class='iui-button-split'>
      <Button_ size='small' disabled>Small split menu</Button_>
      <IconButton_ size='small' disabled>
        <svg-caret-down-small></svg-caret-down-small>
      </IconButton_>
    </span>

    <br />

    <span class='iui-button-split'>
      <Button_ variant='high-visibility'>Split menu</Button_>
      <IconButton_ variant='high-visibility'>
        <svg-caret-down-small></svg-caret-down-small>
      </IconButton_>
    </span>
    <span class='iui-button-split'>
      <Button_ variant='cta'>Split menu</Button_>
      <IconButton_ variant='cta'>
        <svg-caret-down-small></svg-caret-down-small>
      </IconButton_>
    </span>
    <span class='iui-button-split'>
      <Button_>Split menu</Button_>
      <IconButton_>
        <svg-caret-down-small></svg-caret-down-small>
      </IconButton_>
    </span>
    <span class='iui-button-split'>
      <Button_ disabled>Split menu</Button_>
      <IconButton_ disabled>
        <svg-caret-down-small></svg-caret-down-small>
      </IconButton_>
    </span>

    <br />

    <span class='iui-button-split'>
      <Button_ variant='high-visibility' size='large'>Large split menu</Button_>
      <IconButton_ variant='high-visibility' size='large'>
        <svg-caret-down-small></svg-caret-down-small>
      </IconButton_>
    </span>
    <span class='iui-button-split'>
      <Button_ variant='cta' size='large'>Large split menu</Button_>
      <IconButton_ variant='cta' size='large'>
        <svg-caret-down-small></svg-caret-down-small>
      </IconButton_>
    </span>
    <span class='iui-button-split'>
      <Button_ size='large'>Large split menu</Button_>
      <IconButton_ size='large'>
        <svg-caret-down-small></svg-caret-down-small>
      </IconButton_>
    </span>
    <span class='iui-button-split'>
      <Button_ size='large' disabled>Large split menu</Button_>
      <IconButton_ size='large' disabled>
        <svg-caret-down-small></svg-caret-down-small>
      </IconButton_>
    </span>
  </section>

  <br />

  <h3>Input + button combo group</h3>

  <section id='demo-combo-group'>
    <div class='iui-button-group'>
      <Button_>Button 1</Button_>
      <input class='iui-input' />
      <Button_>Button 2</Button_>
    </div>

    <br />

    <div class='iui-button-group'>
      <Button_>Button 1</Button_>
      <input class='iui-input' placeholder='Disabled input' disabled />
      <Button_>Button 2</Button_>
    </div>

    <br />

    <div class='iui-button-group'>
      <div class='iui-input-flex-container' data-iui-disabled='true'>
        <input value='https://itwin.github.io/iTwinUI' disabled />
      </div>
      <Button_ variant='high-visibility'>Copy</Button_>
