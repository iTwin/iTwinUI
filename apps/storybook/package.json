{
  "name": "storybook",
  "version": "0.0.0",
  "private": true,
  "dependencies": {
    "storybook": "~7.0.0-beta.47"
  },
  "devDependencies": {
    "@itwin/itwinui-icons-react": "^1.10.1",
    "@itwin/itwinui-react": "*",
    "@storybook/addon-a11y": "~7.0.0-beta.47",
    "@storybook/addon-actions": "~7.0.0-beta.47",
    "@storybook/addon-essentials": "~7.0.0-beta.47",
    "@storybook/addon-storysource": "~7.0.0-beta.47",
    "@storybook/node-logger": "~7.0.0-beta.47",
    "@storybook/react": "~7.0.0-beta.47",
    "@storybook/react-vite": "~7.0.0-beta.47",
    "@types/node": "^16.11.31",
    "@types/react": "^18.0.2",
    "@types/react-dom": "^18.0.2",
    "configs": "*",
    "cypress": "9.6.0",
    "cypress-image-diff-js": "1.18.1",
    "dotenv-cli": "7.0.0",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-storybook": "^0.6.10",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-table": "^7.1.0",
    "serve": "^14.0.1",
    "storybook-dark-mode": "^1.1.0",
    "typescript": "^4.6.3",
    "vite": "^4.1.0"
  },
  "scripts": {
    "build": "storybook build",
    "dev": "storybook dev -p 6006 --no-open --quiet",
    "lint": "eslint \"**/*.{ts,tsx}\" --max-warnings=0",
    "test": "node scripts/run-tests.js",
    "approve": "cypress-image-diff -u",
    "push-docker": "node scripts/push-docker.js",
    "cy": "cypress open",
    "cy:run": "cypress run",
    "clean": "rimraf .turbo && rimraf node_modules && rimraf storybook-static"
  },
  "lint-staged": {
    "*.{tsx,ts,jsx,js}": [
      "prettier --write",
      "node ../../scripts/copyrightLinter.js --fix"
    ],
    "*.{tsx,ts}": [
      "eslint --max-warnings=0 --fix"
    ]
  },
  "prettier": "configs/prettier-config",
  "browserslist": [
    ">0.5%",
    "not dead",
    "not op_mini all"
  ]
}
