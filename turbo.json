{
  "$schema": "https://turborepo.org/schema.json",
  "baseBranch": "origin/main",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "@itwin/itwinui-css#build": {
      "dependsOn": ["@itwin/itwinui-variables#build"],
      "outputs": ["css/**"]
    },
    "@itwin/itwinui-react#build": {
      "dependsOn": ["^build"],
      "outputs": ["esm/**", "cjs/**"]
    },
    "storybook#build": {
      "dependsOn": ["^build"],
      "outputs": ["storybook-static/**"]
    },
    "test": {
      "outputs": []
    },
    "storybook#test": {
      "dependsOn": ["build"]
    },
    "@itwin/itwinui-css#test": {
      "dependsOn": ["build"]
    },
    "playwright-testing#test": {
      "dependsOn": ["^build"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "dependsOn": [],
      "cache": false
    },
    "format": {
      "outputs": []
    },
    "clean": {
      "outputs": [],
      "cache": false
    }
  }
}
