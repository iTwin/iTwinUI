{
  "$schema": "https://turborepo.org/schema.json",
  "globalDotEnv": [".env"],
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "@itwin/itwinui-css#build": {
      "dependsOn": ["@itwin/itwinui-variables#build"],
      "outputs": ["css/**"]
    },
    "@itwin/itwinui-react#build": {
      "dependsOn": ["^build"],
      "outputs": ["esm/**", "cjs/**"]
    },
    "react-workshop#build": {
      "dependsOn": ["^build"],
      "outputs": ["build/**"]
    },
    "test": {},
    "react-workshop#test": {
      "dependsOn": ["build"],
      "outputs": ["dist/**", "build/**"]
    },
    "css-workshop#test": {
      "dependsOn": ["build"],
      "outputs": ["dist/**", "build/**"]
    },
    "a11y#test": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", "build/**"]
    },
    "lint": {},
    "dev": {
      "dependsOn": [],
      "dotEnv": [".env.development", ".env.local"],
      "cache": false
    },
    "vite-playground#dev": {
      "persistent": true
    },
    "css-workshop#dev": {
      "persistent": true
    },
    "format": {},
    "clean": {
      "outputs": [],
      "cache": false
    }
  }
}
